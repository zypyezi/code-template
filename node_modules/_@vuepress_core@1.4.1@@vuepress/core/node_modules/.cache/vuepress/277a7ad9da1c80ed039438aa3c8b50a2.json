y 里面\n                        const content = JSON.parse(ifr.contentDocument.body.innerText)\n                        console.log(content.msg || '文件下载出错')\n                    } catch(err) {\n                        // TODO\n                    }\n                    document.body.removeChild(ifr)\n                }\n                ifr.id = id\n                ifr.src = this.url\n                ifr.style.display = 'none'\n\n                if (!hasAppend) {\n                    document.body.appendChild(ifr)\n                }\n            } catch (ex) {\n                // TODO\n            } finally {\n                // TODO\n            }\n        }\n    }\n}\n"]}]}