                    document.body.removeChild(ifr)\n                }\n                ifr.id = id\n                ifr.src = this.url\n                ifr.style.display = 'none'\n\n                if (!hasAppend) {\n                    document.body.appendChild(ifr)\n                }\n            } catch (ex) {\n                // TODO\n            } finally {\n                // TODO\n            }\n        }\n    }\n}\n</script>\n\n<style>\n    .input{\n        margin-top: 20px;\n        display: inline-block;\n        width: 100%;\n        height: 32px;\n        padding: 4px 11px;\n        color: rgba(0,0,0,.65);\n        font-size: 14px;\n        background-color: #fff;\n        border: 1px solid #d9d9d9;\n        border-radius: 4px;\n        width: 300px;\n    }\n    .btn{\n        margin-top: 20px;\n        color: #fff;\n        background-color: #1890ff;\n        border-color: #1890ff;\n        text-shadow: 0 -1px 0 rgba(0,0,0,.12);\n        -webkit-box-shadow: 0 2px 0 rgba(0,0,0,.045);\n        box-shadow: 0 2px 0 rgba(0,0,0,.045);\n        text-align: center;\n        display: inline-block;\n        height: 32px;\n        line-height: 32px;\n        padding: 0 15px;\n        border-radius: 4px;\n        font-size: 14px;\n        cursor: pointer;\n    }\n</style>"],"sourceRoot":"docs/.vuepress/components"}]}